    font-size:40px;
    display: flex;
    justify-content: space-between;
    padding: 40px;
    border-bottom: 4px solid #ddd;
    &-coin{
      color: #ff9d3a;
    }
  }

  &-title{
    font-size:40px;
    padding: 40px;
  }

  &-container{
    display: flex;
    justify-content: space-between;
  }
}
