@PrefixCls: fazhandangyuanxinxi;

.@{PrefixCls}-outer{
  min-height: 100vh;
  background: #fff;
}
.@{PrefixCls}-button{
  width: 100%;
  position: fixed;
  bottom:10px;
  left: 0;
}

.@{PrefixCls}-container{
  padding: 0 20px;
  :global{
    .am-list-item .am-list-line .am-list-extra{
      flex-basis: 60%;
    }
  }
}
.@{PrefixCls}-formbox{
  float: right;
  margin-top: 10px;
  padding: 20px;
  border-radius: 50%;
  background: #4eaaf7;
}
.@{PrefixCls}-imgbox{
  width: 100%;
  margin:20px 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap:wrap;
  & div {
    width: 100%;
    height:500px;
    background-size: cover;
    margin-bottom:20px;
  }
}
