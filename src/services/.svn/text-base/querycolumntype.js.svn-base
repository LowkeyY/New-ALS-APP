import { request, config } from 'utils'

const { api } = config
const { QueryColumnTypeApi } = api

export async function queryColumn (payload) {
  return request({
    url: QueryColumnTypeApi,
    method: 'get',
    data: payload,
  })

}

export async function queryNews (payload) {
  return request({
    url: QueryColumnTypeApi,
    method: 'get',
    data: payload,
  })

}
