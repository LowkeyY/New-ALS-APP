@PrefixCls: survey;
.@{PrefixCls}-outer {
  background: #fff;
}

.@{PrefixCls}-item {
  & span {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
  }
  & img {
    width: 180px;
    height: 140px;
  }
}

.@{PrefixCls}-fixeditem {
  & img {
    width: 100px;
    height: 100px;
  }
}

.@{PrefixCls}-list-title {
  font-size: 35px;
  color: black;
  font-weight: bolder;
}

.@{PrefixCls}-card-title {
  font-size: 40px;
  font-weight: bolder;
}
